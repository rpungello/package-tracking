{
    "name": "rpungello/package-tracking",
    "description": "Provides helpful utilities for dealing with tracking numbers",
    "keywords": [
        "rpungello",
        "package-tracking"
    ],
    "homepage": "https://github.com/rpungello/package-tracking",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Pungello",
            "email": "rob.pungello@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ramsey/collection": "^1.2"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rpungello\\PackageTracking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rpungello\\PackageTracking\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
